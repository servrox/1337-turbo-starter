{
  "name": "turbo-bun-tailwind-aptos-starter",
  "private": true,
  "author": "servrox",
  "scripts": {
    "build": "turbo run build",
    "build:aptos-boilerplate": "turbo run build --filter=aptos-boilerplate",
    "build:landing-page": "turbo run build --filter=landing-page",
    "build:wallet-dashboard": "turbo run build --filter=wallet-dashboard",
    "dev": "turbo run dev",
    "dev:aptos-boilerplate": "turbo run dev --filter=aptos-boilerplate",
    "dev:landing-page": "turbo run dev --filter=landing-page",
    "dev:wallet-dashboard": "turbo run dev --filter=wallet-dashboard",
    "dev:wallet_dashboard": "bun run dev:wallet-dashboard",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "test:aptos-boilerplate": "turbo run test --filter=aptos-boilerplate",
    "test:landing-page": "turbo run test --filter=landing-page",
    "test:wallet-dashboard": "turbo run test --filter=wallet-dashboard",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/minimatch": "^6.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "turbo": "^2.6.1"
  },
  "engines": {
    "bun": ">=1.2.19"
  },
  "packageManager": "bun@1.3.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "@vercel/speed-insights",
    "bigint-buffer",
    "bufferutil",
    "core-js-pure",
    "es5-ext",
    "esbuild",
    "keccak",
    "node-hid",
    "protobufjs",
    "secp256k1",
    "sharp",
    "usb",
    "utf-8-validate"
  ]
}
