{
  "name": "iota-wallet-dashboard",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=\"development\" next dev",
    "build": "cross-env NODE_ENV=\"production\" next build",
    "start": "next start",
    "lint": "eslint --max-warnings=0 . && bun run prettier:check",
    "prettier:check": "prettier -c --ignore-unknown --ignore-path=../../.prettierignore --ignore-path=.prettierignore .",
    "prettier:fix": "prettier -w --ignore-unknown --ignore-path=../../.prettierignore --ignore-path=.prettierignore .",
    "test": "jest",
    "ampli": "ampli",
    "pull-amplitude": "ampli pull web && node prependLicense.mjs && prettier -w ampli.json lib/utils/analytics/ampli/index.ts",
    "prepare:e2e": "bun run build",
    "playwright": "playwright",
    "playwright:debug": "PWDEBUG=1 playwright"
  },
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.31.3",
    "@amplitude/analytics-types": "^2.11.0",
    "@boxfish-studio/react-cookie-manager": "^1.0.5",
    "@growthbook/growthbook": "^1.6.2",
    "@growthbook/growthbook-react": "^1.6.2",
    "@hookform/resolvers": "^5.2.2",
    "@iota/apps-ui-icons": "^0.7.0",
    "@iota/apps-ui-kit": "^0.9.0",
    "@iota/dapp-kit": "^0.8.0",
    "@iota/iota-names-sdk": "^0.4.0",
    "@iota/iota-sdk": "^1.9.0",
    "@iota/kiosk": "^0.6.0",
    "@iota/wallet-standard": "^0.3.0",
    "@repo/iota-core": "workspace:*",
    "@repo/ui": "workspace:*",
    "@sentry/nextjs": "^10.29.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-virtual": "^3.13.12",
    "bignumber.js": "^9.3.1",
    "clsx": "^2.1.1",
    "formik": "^2.4.9",
    "idb-keyval": "^6.2.2",
    "next": "16.0.7",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.1",
    "react-hot-toast": "^2.6.0",
    "vanilla-cookieconsent": "^3.1.0",
    "yup": "^1.7.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@amplitude/ampli": "^1.36.2",
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
